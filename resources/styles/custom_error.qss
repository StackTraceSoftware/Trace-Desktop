QMessageBox {
    background-color: rgb(40, 39, 46);
    border: 2px solid rgb(40, 39, 46);
    border-radius: 10px;
}
QLabel {
    color: white;
    font-size: 16px;
    font-weight: bold;
    padding: 10px;
}
QPushButton {
    background-color: #0078d7;
    color: white;
    border: none;
    border-radius: 5px;
    padding: 5px 10px;
}
QPushButton:hover {
    background-color: #0056a1;
}
